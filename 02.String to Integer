import java.util.*;
public class e2 
{
	public static void main(String args[])
	{
		String input1;
		int input2;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a String:");
		input1=sc.nextLine();
		try
		{
			input2=Integer.parseInt(input1);
			System.out.println("After string converted into integer:" + input2);		
		}
		catch(Exception e)
		{
			System.out.println("Invalid input!, Please string to convert integer");
		}
		
	}
}
