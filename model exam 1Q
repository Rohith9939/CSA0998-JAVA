package pattern1;
import java.util.*;
class sample1
{
public static void main(String[] args)
{
int num;
Scanner s = new Scanner(System.in);
System.out.print("Enter the Row ");
int n = s.nextInt();
for(int i=0; i<n; i++)
{
for(int j=n; j>i; j--)
System.out.print(" ");
num=1;
for(int j=0; j<=i; j++)
{
System.out.print(num+ " ");
num = num*(i-j)/(j+1);
}
System.out.println();
}
}
}
